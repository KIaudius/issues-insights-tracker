[run]
source = app
omit = 
    */tests/*
    */migrations/*
    */alembic/*
    */__init__.py
    */config.py
    */main.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    
[html]
directory = htmlcov
